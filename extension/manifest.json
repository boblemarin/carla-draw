{
  "manifest_version": 2,
  "name": "Carla Draw",
  "description": "Allows drawing on existing images and saving a nice picture (in SVG format)",
  "version": "0.1.1",
  "icons": { "128": "icon128.png" },
  "permissions": [ "activeTab" ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}