{
  "manifest_version": 2,
  "name": "Carla Draw",
  "description": "Allows drawing on existing images and saving as SVG file",
  "version": "0.1",
  "icons": { "128": "icon128.png" },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],

  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}